<odoo>
  <data>
    <record id="master_spbu_view_tree" model="ir.ui.view">
      <field name="name">master_spbu.tree</field>
      <field name="model">master_spbu.premium</field>
      <field name="arch" type="xml">
        <tree>
          <field name="name"/>
          <field name="tanggal"/>
        </tree>
      </field>
    </record>

    <record id="master_spbu_view_tree2" model="ir.ui.view">
      <field name="name">master_spbu.tree2</field>
      <field name="model">master_spbu.premium2</field>
      <field name="arch" type="xml">
        <tree>
          <field name="name"/>
          <field name="tanggal"/>
        </tree>
      </field>
    </record>



    <record id='master_spbu_view_form' model='ir.ui.view'>
      <field name="name">master_spbu.form</field>
      <field name="model">master_spbu.premium</field>
      <field name="arch" type="xml">
        <form string="Report">
          <sheet>
            <group>
              <field name="name"/>
              <field name="tanggal"/>
            </group>
            <notebook>
              <page string="Product">
                <field name="premium_line_ids" nonlabel="1" options="{'reload_on_button': true}">
                  <tree string="Product" editable="bottom" edit="true" create="true" delete="true">
                    <field name="tanggal"/>
                    <field name="stock_awal"/>
                    <field name="kiriman"/>
                    <field name="tera"/>
                    <field name="t1"/>
                    <field name="t2"/>
                    <field name="t3"/>
                    <field name="t4"/>
                    <field name="laku"/>
                    <field name="rupiah"/>
                    <field name="stock_akhir"/>
                    <field name="stock_tangki"/>
                    <field name="selisih"/>
                    <field name="akumulasi"/>
                  </tree>
                </field>
              </page>
            </notebook>
          </sheet>
        </form>
        </field>
    </record>
    
    <record id='master_spbu_view_form2' model='ir.ui.view'>
      <field name="name">master_spbu.form2</field>
      <field name="model">master_spbu.premium2</field>
      <field name="arch" type="xml">
        <form string="Report">
          <sheet>
            <group>
              <field name="name"/>
              <field name="tanggal"/>
            </group>
            <notebook>
              <page string="Product">
                <field name="premium2_line_ids" nonlabel="1" options="{'reload_on_button': true}">
                  <tree string="Product" editable="bottom" edit="true" create="true" delete="true">
                    <field name="doa_awal"/>
                    <field name="tebusan"/>
                    <field name="kiriman"/>
                    <field name="sisa_do"/>
                  </tree>
                </field>
              </page>
            </notebook>
          </sheet>
        </form>
        </field>
    </record>
  </data>
</odoo>
